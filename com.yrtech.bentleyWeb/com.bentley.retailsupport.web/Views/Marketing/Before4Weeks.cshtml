        var UserId = $("#G_UserId").val();
        let imgCodeObj = { "MPF01": "imgListDiv", "MPF01-1": "imgListDiv-1", "MPF02": "imgListDiv-2", "MPF03": "imgListDiv-3", "MPF04": "imgListDiv-4", "MPF05": "imgListDiv-5", "MPF06": "imgListDiv-6", "MPF07": "imgListDiv-7", "MPF08": "imgListDiv-8", "MPF09": "imgListDiv-9", "MPF12": "imgListDiv-10"};
        $("#nav_promotion").addClass("active");

        var baseOss = "https://yrsurvey.oss-cn-beijing.aliyuncs.com/";
        var id = '@ViewBag.Id';
        var hdData = [];
        var hdDataCoop = [{ "text": "是", "value": "true" }, { "text": "否", "value": "false" }];

        $('#UserId').val($('#G_UserId').val());
        var roleName = $('#G_RoleTypeCode').val();
        $(function () {
            initOssClient("");
            initOssClient("-1");
            initOssClient("-2");
            initOssClient("-3");
            initOssClient("-4");
            initOssClient("-5");
            initOssClient("-6");
            initOssClient("-7");
            initOssClient("-8");
            initOssClient("-9");
            initOssClient("-10");
            $('img').error(function () {
                console.log("image error")
                $(this).attr('src', 'https://yrsurvey.oss-cn-beijing.aliyuncs.com/Bentley/fail2.png');
            });

            loadShops().forEach(function (item) {
                var shopName = localStorage.language == 'en' ? item.ShopNameEn : item.ShopName;
                $("#ShopId").append($("<option>").val(item.ShopId).text(shopName));
            });
            //设置同步调用 必须先绑定下拉列表数据再查询市场活动信息
            $.ajaxSetup({
                async: false
            });
            loadActiveStatuss();
            loadTargetModels();
            loadEventTypes();
            initAjax();
            $.ajaxSetup({
                async: true
            });

            InitActivityFlowTable();
            InitActivityFlowTableNew();

            if (roleName == "SYSADMIN" || roleName == "BMC") {
                $('#ActiveShow').show();
                $('#VisualStatus').prop("disabled", false);
                $('#VisualDescription').prop("disabled", false);
            }
            else {
                $('#VisualStatus').prop("disabled", true);
                $('#VisualDescription').prop("disabled", true);
            }

            if (id) {
                $.commonGet("MarketAction/MarketActionSearchById", { marketActionId: id }, function (data) {
                    if (data && data.length > 0) {
                        data[0].StartDate = data[0].StartDate.substr(0, data[0].StartDate.indexOf(' ')).replace(/-/g, '\/');
                        data[0].EndDate = data[0].EndDate.substr(0, data[0].EndDate.indexOf(' ')).replace(/-/g, '\/');
                        $('#PromotionCreate').setForm(data[0]);
                        $("#MarketActionTargetModelCode").val(data[0].MarketActionTargetModelCode);
                        $("select").selectpicker("refresh");
                    }
                });

                $.commonGet("MarketAction/MarketActionBefore4WeeksSearch", { marketActionId: id }, function (data) {
                    if (data) {
                        $('#PromotionCreate2').setForm(data.MarketActionBefore4Weeks);
                        $('#ActivityFlowTable').bootstrapTable("load", data.ActivityProcess);
                        $('#ActivityFlowTableNew').bootstrapTable("load", data.MarketActionBefore4WeeksCoopFund);
                        if (data.ActivityProcess) {
                            data.ActivityProcess.forEach(function (item) {
                                if (item.SeqNO > maxActivityFlowSeqNO) {
                                    maxActivityFlowSeqNO = item.SeqNO;
                                }
                            });
                        }
                        if (data.MarketActionBefore4WeeksCoopFund) {
                            data.MarketActionBefore4WeeksCoopFund.forEach(function (item) {
                                if (item.SeqNO > maxActivityFlowSeqNONew) {
                                    maxActivityFlowSeqNONew = item.SeqNO;
                                }
                            });
                        }
                        initImageList(data.MarketActionBefore4WeeksPicList_OffLine);
                        $("#VisualStatus").selectpicker("refresh");
                        $('#img_visual1').fileinputInit(data.MarketActionBefore4Weeks.KeyVisionPic);
                        $("#VisualStatusOld").val(data.MarketActionBefore4Weeks.KeyVisionApprovalCode);
                        $("#VisualDescriptionOld").val(data.MarketActionBefore4Weeks.KeyVisionApprovalDesc);
                    }

                });
            }

        });

        function SProcessing() {
            //loading
            var index = layer.load(1, {
                shade: [0.1, '#fff'] //0.1透明度的白色背景
