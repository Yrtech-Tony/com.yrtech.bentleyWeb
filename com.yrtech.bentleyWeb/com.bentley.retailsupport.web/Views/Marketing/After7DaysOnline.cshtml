        //权限
        function initRole() {
            if (roleName == "SYSADMIN") {
                return;
            }
            let _dttCode = $('#DTTApproveCode').val();
            console.error(_dttCode);
            if (_dttCode == 2) {
                $("#btnSave").addClass("btn-set-the-ash");
                $("#btnSubmit").addClass("btn-set-the-ash");
            }
            //DTT权限
            if (roleName == "AREA") {
                $('#DTTApproveDesc').prop("disabled", false);
                $('#DTTApproveCode').prop("disabled", false);
            }
            //主视觉权限
            if (roleName == "BMC") {
                $('#DTTApproveDesc').prop("disabled", true);
                $('#DTTApproveCode').prop("disabled", true);
            }
            //经销商权限
            if (roleName == "SHOP") {
                //主视觉权限
                $('#DTTApproveDesc').prop("disabled", true);
                $('#DTTApproveCode').prop("disabled", true);
                //DTT审批通过-主视觉审批通过
                if (_dttCode == 2) {
                    //都不可修改
                    //全部input框
                    $('.form-control').prop("disabled", true);
                    //表格点击事件
                    $(".table-unedit").css({ "pointer-events": "none" });
                    //主视觉图片
                    $("#picID").css({ "pointer-events": "none" });
                    //图片上传
                    for (let key in imgCodeObj) {
                        let _imgList = $("#" + imgCodeObj[key]).find(".ossImage");
                        for (let i = 0; i < _imgList.length; i++) {
                            $(_imgList[i]).parent().parent().find(".fileinput-exists").hide();
                            $(_imgList[i]).parent().parent().parent().children('div:last-child').hide();
                        }
                    }
                }
            }
        }

        function searchDtt() {
            $.commonGet("MarketAction/DTTApproveSearch", {
                marketActionId: $("#MarketActionId").val(),
                dttType: 2,
                dttApproveId: '',
                dttApproveCode: ''
            }, function (data) {
                if (data && data.length > 0) {
                    $("#DTTApproveCode").val(data[0].DTTApproveCode);
                    $("#DTTApproveDesc").val(data[0].DTTApproveDesc);
                }
                $("#DTTApproveCode").selectpicker("refresh");
                initRole();
            });
        }

        function saveCoopFund(_item) {
            //市场基金申报信息
            var StrActivityFlowNew = $('#ActivityFlowTableOnline').bootstrapTable('getData');
            let _flowDateVeri = true;
            let _flowVeri = true;
            let _CoopFundSumAmt = $("#CoopFundSumAmt").val();
            @*for (let _key in _item) {
                if (_key == "CoopFundCode" && !_item[_key]) {
                    _flowVeri = false;
                    return;
                }
                if (_key == "CoopFundAmt" && !_item[_key]) {
                    _flowVeri = false;
                    return;
                }
                if (_key == "CoopFund_DMFChk" && !_item[_key]) {
                    _flowVeri = false;
                    return;
                }
                if (_key == "StartDate" && !_item[_key]) {
                    _flowVeri = false;
                    return;
                }
                if (_key == "EndDate" && !_item[_key]) {
                    _flowVeri = false;
                    return;
                }
            }*@
            let _repetitionType = true;
            StrActivityFlowNew.forEach(function (item) {
                if (item.SeqNO != _item.SeqNO && item.CoopFundCode == _item.CoopFundCode) {
                    _repetitionType = false;
                }
            });
            if (!_repetitionType) {
                layer.open({
                    title: '提示',
                    type: 0,
                    content: '费用类型不可重复，请重新选择！',
                    end: function () {
                        StrActivityFlowNew.forEach(function (item) {
                            if (item.SeqNO == _item.SeqNO) {
                                item.CoopFundCode = '';
                            }
                        });
                        $('#ActivityFlowTableOnline').bootstrapTable("load", StrActivityFlowNew);
                    }
                });
                return;
            }
            //验证结束时间大于开始时间
            if (_item["StartDate"] && _item["EndDate"]) {
                let _startDate = new Date(_item["StartDate"]);
                let _endDate = new Date(_item["EndDate"]);
                if (_startDate.getTime() > _endDate.getTime()) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '投放开始时间不能大于等于投放结束时间！'
                    });
                    _item.EndDate = '';
                    StrActivityFlowNew.forEach(function (item) {
                        if (item.SeqNO == _item.SeqNO) {
                            item = _item;
                        }
                    });
                    $('#ActivityFlowTableOnline').bootstrapTable("load", StrActivityFlowNew);
                    return;
                }
            }
            let _sumAmt = 0;
            _item.MarketActionId = id;
            SProcessing();
            $.commonPost("MarketAction/MarketActionAfter7CoopFundSave", _item, function (data) {
                if (data) {
                    _sumAmt = 0;
                    StrActivityFlowNew.forEach(function (item) {
                        if (item.SeqNO == _item.SeqNO) {
                            item.TotalDays = data.TotalDays;
                            item.AmtPerDay = data.AmtPerDay;
                            item.StartDate = data.StartDate;
                            item.EndDate = data.EndDate
                            item.CoopFundAmt = data.CoopFundAmt;
                            item.CoopFund_DMFChk = data.CoopFund_DMFChk;
                            item.CoopFundDesc = data.CoopFundDesc;
                            item.CoopFundTypeDesc = data.CoopFundTypeDesc;
                            item.CoopFundAmt_Budget = data.CoopFundAmt_Budget;
                        }
                        //计算金额
                        if (item.CoopFund_DMFChk) {
                            _sumAmt += parseInt(item.CoopFundAmt);
                        }
                    });
                    $("#TotalBudgetAmt").val(_sumAmt);
                    subCluesToTheCostReport();
                }
                $('#ActivityFlowTableOnline').bootstrapTable("load", StrActivityFlowNew);
                EProcessing();
            }, EProcessing)

        }

