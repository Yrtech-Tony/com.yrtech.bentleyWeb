    <script>
        $("#nav_promotion").addClass("active");
        let imgCodeObj = { "MRN01": "imgListDiv", "MRN04": "imgListDiv-1", "MRN02": "imgListDiv-2", "MRN05": "imgListDiv-3", "MRN03": "imgListDiv-4", "MPN10": "imgListDiv-5", "MRN06": "imgListDiv-6" };
        var baseOss = "https://yrsurvey.oss-cn-beijing.aliyuncs.com/";
        var id = '@ViewBag.Id';
        var hdData = [];
        var hdDataCoop = [{ "text": "是", "value": "true" }, { "text": "否", "value": "false" }];

        $(function () {
            $('img').error(function () {
                $(this).attr('src', 'https://yrsurvey.oss-cn-beijing.aliyuncs.com/Bentley/fail2.png');
            });

            loadShops().forEach(function (item) {
                var shopName = localStorage.language == 'en' ? item.ShopNameEn : item.ShopName;
                $("#ShopId").append($("<option>").val(item.ShopId).text(shopName));
            });
            initOssClient("");
            initOssClient("-1");
            initOssClient("-2");
            initOssClient("-3");
            initOssClient("-4");
            initOssClient("-6");
            initVideoOssClient("-5");
            //设置同步调用 必须先绑定下拉列表数据再查询市场活动信息
            $.ajaxSetup({
                async: false
            });
            loadActiveStatuss();
            loadTargetModels();
            loadEventTypes();
            initAjax();
            $.ajaxSetup({
                async: true
            });

            InitActivityFlowTableOnline();

            $('#UserId').val($('#G_UserId').val());
            var roleName = $('#G_RoleTypeCode').val();
            if (roleName == "SYSADMIN" || roleName == "MARKET") {
                $('#ActiveShow').show();
                $('#VisualStatus').prop("disabled", false);
            }
            else {
                $('#VisualStatus').prop("disabled", true);
            }

            if (id) {
                $.commonGet("MarketAction/MarketActionSearchById", { marketActionId: id }, function (data) {
                    if (data && data.length > 0) {
                        data[0].StartDate = data[0].StartDate.substr(0, data[0].StartDate.indexOf(' ')).replace(/-/g, '\/');
                        data[0].EndDate = data[0].EndDate.substr(0, data[0].EndDate.indexOf(' ')).replace(/-/g, '\/');
                        $('#PromotionCreate').setForm(data[0]);
                        $("#MarketActionTargetModelCode").val(data[0].MarketActionTargetModelCode);
                        $("select").selectpicker("refresh");
                    }
                });

                $.commonGet("MarketAction/MarketActionAfter7Search", { marketActionId: id }, function (data) {
                    if (data) {
                        var marketActionAfter7 = data.MarketActionAfter7;
                        if (data.MarketActionAfter7CoopFund) {
                            data.MarketActionAfter7CoopFund.forEach(function (item) {
                                if (item.SeqNO > maxActivityFlowSeqNOOnline) {
                                    maxActivityFlowSeqNOOnline = item.SeqNO;
                                }
                            });
                            $('#ActivityFlowTableOnline').bootstrapTable("load", data.MarketActionAfter7CoopFund);
                        }
                        if (marketActionAfter7) {
                            $('#PromotionCreate2').setForm(marketActionAfter7);
                            if (data.MarketActionBefore4Weeks) {
                                $('#PromotionCreate2').setForm(data.MarketActionBefore4Weeks);
                            }
                            initImageList(data.MarketActionAfter7PicList_OnLine);
                        }

                    }
                });
            }
        });

        function SProcessing() {
            //loading
            var index = layer.load(1, {
                shade: [0.1, '#fff'] //0.1透明度的白色背景
            });
        }
        function EProcessing() {
            layer.closeAll('loading');
        }

        function PromotionEdit2() {
            if (!veriData(1)) {
                return;
            }

            var marketActionAfter7 = $('#PromotionCreate2').serializeObject();
            marketActionAfter7.MarketActionId = id;
            marketActionAfter7.InUserId = $("#G_UserId").val();
            marketActionAfter7.ModifyUserId = $("#G_UserId").val();

            let MarketActionBefore4WeeksPicList_OffLine = [];
            for (let _key in imgCodeObj) {
                let _offLineList = submitImage(_key);
                if (_offLineList && _offLineList.length > 0) {
                    MarketActionBefore4WeeksPicList_OffLine = MarketActionBefore4WeeksPicList_OffLine.concat(_offLineList);
                }
            }
            marketActionAfter7.ProcessPercent = planProgessSub();
            var StrActivityFlowNew = $('#ActivityFlowTableOnline').bootstrapTable('getData');
            if (StrActivityFlowNew && StrActivityFlowNew.length > 0) {
                StrActivityFlowNew.forEach(function (item) {
                    item.MarketActionId = id;
                })
            }
            var data = {
                MarketActionId: id,
                MarketActionAfter7: marketActionAfter7,
                MarketActionAfter7CoopFund: StrActivityFlowNew,
                MarketActionAfter7PicList_OnLine: MarketActionBefore4WeeksPicList_OffLine
            };

            SProcessing();
            $.commonPost("MarketAction/MarketActionAfter7Save", {
                UserId: $("#G_UserId").val(),
                ListJson: JSON.stringify(data)
            }, function () {
                EProcessing();
                var msg = isZH() ? '保存成功！' : "Save successfully !";
                layer.alert(msg, {
                    skin: 'layui-layer-molv', closeBtn: 0
                }, function (datas) {
                    window.location.href = "/Marketing/Index";
                });
            }, EProcessing)
        }

        function PromotionSubmit() {
            if (!veriData(1)) {
                return;
            }

            var marketActionAfter7 = $('#PromotionCreate2').serializeObject();
            marketActionAfter7.MarketActionId = id;
            marketActionAfter7.InUserId = $("#G_UserId").val();
            marketActionAfter7.ModifyUserId = $("#G_UserId").val();

            let MarketActionBefore4WeeksPicList_OffLine = [];
            for (let _key in imgCodeObj) {
                let _offLineList = submitImage(_key);
                if (_offLineList && _offLineList.length > 0) {
                    MarketActionBefore4WeeksPicList_OffLine = MarketActionBefore4WeeksPicList_OffLine.concat(_offLineList);
