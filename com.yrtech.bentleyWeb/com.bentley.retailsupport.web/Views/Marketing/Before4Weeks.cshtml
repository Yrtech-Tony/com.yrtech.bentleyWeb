        function addImage(_id, args) {
            let imgPath = args.osspath;
            let fileName = args.fileName;
            let _imgList = $("#imgListDiv" + _id).find(".ossImage");
            for (let i = 0; i < _imgList.length; i++) {
                if ($(_imgList[i]).attr("data-flag") == 1) {
                    $(_imgList[i]).attr("src", baseOss + imgPath);
                    $(_imgList[i]).attr("data-ossPath", imgPath);
                    $(_imgList[i]).attr("data-flag", 2);
                    $(_imgList[i]).attr("data-fileName", fileName);
                    $(_imgList[i]).parent().parent().find(".fileinput-exists").show();
                    return;
                }
            }
        }

        function delImage(_delMode) {
            $(_delMode).parent().parent().find(".ossImage").attr("src", "/Content/bootstrap-fileinput/img/noimage.png");
            $(_delMode).parent().parent().find(".ossImage").attr("data-flag", 1);
            $(_delMode).hide();
            
