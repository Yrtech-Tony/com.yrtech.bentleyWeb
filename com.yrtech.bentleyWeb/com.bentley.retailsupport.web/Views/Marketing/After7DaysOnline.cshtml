        let imgCodeObj = { "MRN01": "imgListDiv", "MRN04": "imgListDiv-1", "MRN02": "imgListDiv-2", "MRN05": "imgListDiv-3", "MRN03": "imgListDiv-4", "MRN06": "imgListDiv-6", "MRN10":"imgListDiv-5" };
        var baseOss = "https://yrsurvey.oss-cn-beijing.aliyuncs.com/";
        var id = '@ViewBag.Id';
        var hdData = [];
        var hdDataCoop = [{ "text": "是", "value": "true" }, { "text": "否", "value": "false" }];
        var roleName = $('#G_RoleTypeCode').val();
        $(function () {
            $('img').error(function () {
                $(this).attr('src', 'https://yrsurvey.oss-cn-beijing.aliyuncs.com/Bentley/fail2.png');
            });

            loadShops().forEach(function (item) {
                var shopName = localStorage.language == 'en' ? item.ShopNameEn : item.ShopName;
                $("#ShopId").append($("<option>").val(item.ShopId).text(shopName));
            });
            initOssClient("");
            initOssClient("-1");
            initOssClient("-2",20);
            initOssClient("-3");
            initOssClient("-4",20);
            initOssClient("-6");
            initVideoOssClient("-5",2);
            //设置同步调用 必须先绑定下拉列表数据再查询市场活动信息
            $.ajaxSetup({
                async: false
            });
            loadActiveStatuss();
            loadTargetModels();
            loadEventTypes();
            initAjax();
            $.ajaxSetup({
                async: true
            });

            InitActivityFlowTableOnline(2);

            $('#UserId').val($('#G_UserId').val());
            if (roleName == "SYSADMIN" || roleName == "MARKET") {
                $('#ActiveShow').show();
                $('#VisualStatus').prop("disabled", false);
            }
            else {
                $('#VisualStatus').prop("disabled", true);
            }

            if (id) {
                $.commonGet("MarketAction/MarketActionSearchById", { marketActionId: id }, function (data) {
                    if (data && data.length > 0) {
                        data[0].StartDate = data[0].StartDate.substr(0, data[0].StartDate.indexOf(' ')).replace(/-/g, '\/');
                        data[0].EndDate = data[0].EndDate.substr(0, data[0].EndDate.indexOf(' ')).replace(/-/g, '\/');
                        $("#shopId").val(data[0].ShopId);
                        $('#PromotionCreate').setForm(data[0]);
                        $("#MarketActionTargetModelCode").val(data[0].MarketActionTargetModelCode);
                        $("select").selectpicker("refresh");
                        checkPrice();
                    }
                });

                $.commonGet("MarketAction/MarketActionAfter7Search", { marketActionId: id }, function (data) {
                    if (data) {
                        console.error(data);
                        var marketActionAfter7 = data.MarketActionAfter7;
                        if (data.MarketActionAfter7CoopFund) {
                            data.MarketActionAfter7CoopFund.forEach(function (item) {
                                if (item.SeqNO > maxActivityFlowSeqNOOnline) {
                                    maxActivityFlowSeqNOOnline = item.SeqNO;
                                }
                            });
                            $('#ActivityFlowTableOnline').bootstrapTable("load", data.MarketActionAfter7CoopFund);
                        }
                        if (marketActionAfter7) {
                            $('#PromotionCreate2').setForm(marketActionAfter7);
                            if (data.MarketActionBefore4Weeks) {
                                $('#PromotionCreate2').setForm(data.MarketActionBefore4Weeks);
                            }
                            initImageList(data.MarketActionAfter7PicList_OnLine);
                            subCluesToTheCostReport();
                        }
                        searchDtt();
                        searchFourWeeks(id);
                    }
                });
            }
        });

        function SProcessing() {
            //loading
            var index = layer.load(1, {
                shade: [0.1, '#fff'] //0.1透明度的白色背景
            });
        }
        function EProcessing() {
            layer.closeAll('loading');
        }

        function PromotionEdit2(_type) {
            $('.form-control').prop("disabled", false);
            $('#DTTApproveCode').prop("disabled", false);
            if (!veriData(1)) {
                initRole();
                return;
            }

            var marketActionAfter7 = $('#PromotionCreate2').serializeObject();
            marketActionAfter7.MarketActionId = id;
            marketActionAfter7.InUserId = $("#G_UserId").val();
            marketActionAfter7.ModifyUserId = $("#G_UserId").val();

            let MarketActionBefore4WeeksPicList_OffLine = [];
            for (let _key in imgCodeObj) {
                let _offLineList = submitImage(_key);
                if (_offLineList && _offLineList.length > 0) {
                    MarketActionBefore4WeeksPicList_OffLine = MarketActionBefore4WeeksPicList_OffLine.concat(_offLineList);
                }
            }
            marketActionAfter7.ProcessPercent = planProgessSub();
            var StrActivityFlowNew = $('#ActivityFlowTableOnline').bootstrapTable('getData');
            if (StrActivityFlowNew && StrActivityFlowNew.length > 0) {
                StrActivityFlowNew.forEach(function (item) {
                    item.MarketActionId = id;
                })
            }
            var data = {
                MarketActionId: id,
                MarketActionAfter7: marketActionAfter7,
                MarketActionAfter7CoopFund: StrActivityFlowNew,
                MarketActionAfter7PicList_OnLine: MarketActionBefore4WeeksPicList_OffLine
            };

            SProcessing();
            $.commonPost("MarketAction/MarketActionAfter7Save", {
                UserId: $("#G_UserId").val(),
                ListJson: JSON.stringify(data)
            }, function () {
                EProcessing();
                $('.form-control').prop("disabled", true);
                $('#DTTApproveCode').prop("disabled", true);
                if (_type == 2) {
                    createPPT(1);
                } else {
                    var msg = isZH() ? '保存成功！' : "Save successfully !";
                    layer.alert(msg, {
                        skin: 'layui-layer-molv', closeBtn: 0
                    }, function (datas) {
                        window.location.href = "/Marketing/Index";
                    });
                }
            }, EProcessing)
        }

        function PromotionSubmit() {
            $('.form-control').prop("disabled", false);
            $('#DTTApproveCode').prop("disabled", false);
            if (!veriData(2)) {
                initRole();
                return;
            }

            var marketActionAfter7 = $('#PromotionCreate2').serializeObject();
            marketActionAfter7.MarketActionId = id;
            marketActionAfter7.InUserId = $("#G_UserId").val();
            marketActionAfter7.ModifyUserId = $("#G_UserId").val();

            let MarketActionBefore4WeeksPicList_OffLine = [];
            for (let _key in imgCodeObj) {
                let _offLineList = submitImage(_key);
                if (_offLineList && _offLineList.length > 0) {
                    MarketActionBefore4WeeksPicList_OffLine = MarketActionBefore4WeeksPicList_OffLine.concat(_offLineList);
                }
            }
            marketActionAfter7.ProcessPercent = planProgessSub();
            var StrActivityFlowNew = $('#ActivityFlowTableOnline').bootstrapTable('getData');
            if (StrActivityFlowNew && StrActivityFlowNew.length > 0) {
                StrActivityFlowNew.forEach(function (item) {
                    item.MarketActionId = id;
                })
            }
            var data = {
                MarketActionId: id,
                MarketActionAfter7: marketActionAfter7,
                MarketActionAfter7CoopFund: StrActivityFlowNew,
                MarketActionAfter7PicList_OnLine: MarketActionBefore4WeeksPicList_OffLine
            };

            SProcessing();
            $.commonPost("MarketAction/MarketActionAfter7Save", {
                UserId: $("#G_UserId").val(),
                ListJson: JSON.stringify(data)
            }, function () {
                EProcessing();
                createPPT();
                submitAjax();
            }, EProcessing)
        }

        function OpenForm(obj) {
            var url = obj.src;
            url = url.replace('_min.', '.').replace("?x-oss-process=image/resize,h_100", "");
            if (!new RegExp('Content/bootstrap-fileinput/img/noimage').test(url)) {
                window.open(url);
            }
        }

        function initOssClient(_id,_count) {
            var osspath = "Bentley/" + year + "/MarketAction/";
            ossClient = new OSSClient({
                uploadId: _id,
                imgCount: _count,
                fileAddCheck: function () {
                    return true;
                },
                fileAddCheckMsg: "",
                osspath: osspath,
                uploaded: function (args) {
                    addImage(_id, args);
                }
            });
        }

        function initVideoOssClient(_id,_count) {
            var osspath = "Bentley/" + year + "/MarketAction/";
            ossClient = new OSSClient({
                isVideo:true,
                uploadId: _id,
                imgCount: _count,
                fileAddCheck: function () {
                    return true;
                },
                fileAddCheckMsg: "",
                osspath: osspath,
                uploaded: function (args) {
                    addVideo(_id, args);
                }
            });
        }

        function addImage(_id, args) {
            let imgPath = args.osspath;
            let fileName = args.fileName;
            let _imgList = $("#imgListDiv" + _id).find(".ossImage");
            for (let i = 0; i < _imgList.length; i++) {
                if ($(_imgList[i]).attr("data-flag") == 1) {
                    $(_imgList[i]).attr("src", baseOss + imgPath);
                    $(_imgList[i]).attr("data-ossPath", imgPath);
                    $(_imgList[i]).attr("data-flag", 2);
                    $(_imgList[i]).attr("data-fileName", fileName);
                    $(_imgList[i]).parent().parent().find(".fileinput-exists").show();
                    return;
                }
            }
        }

        function addVideo(_id, args) {
            let imgPath = args.osspath;
            let fileName = args.fileName;
            let _imgList = $("#imgListDiv" + _id).find(".ossImage");
            for (let i = 0; i < _imgList.length; i++) {
                if ($(_imgList[i]).attr("data-flag") == 1) {
                    $(_imgList[i]).removeAttr("poster");
                    $(_imgList[i]).attr("src", baseOss + imgPath);
                    $(_imgList[i]).attr("data-ossPath", imgPath);
                    $(_imgList[i]).attr("data-flag", 2);
                    $(_imgList[i]).attr("data-fileName", fileName);
                    $(_imgList[i]).parent().parent().find(".fileinput-exists").show();
                    return;
                }
            }
        }

        function delImage(_delMode) {
            $(_delMode).parent().parent().find(".ossImage").attr("src", "/Content/bootstrap-fileinput/img/noimage.png");
            $(_delMode).parent().parent().find(".ossImage").attr("poster", "/Content/bootstrap-fileinput/img/novideo.png");
            $(_delMode).parent().parent().find(".ossImage").attr("data-flag", 1);
            $(_delMode).hide();

        }

        function submitImage(_key) {
            let _imgList = $("#" + imgCodeObj[_key]).find(".ossImage");
            let _MarketActionPicList = [];
            for (let i = 0; i < _imgList.length; i++) {
                if ($(_imgList[i]).attr("data-flag") == 2) {
                    let _MarketActionPic = {};
                    _MarketActionPic["PicType"] = _key;
                    _MarketActionPic["PicName"] = $(_imgList[i]).attr("data-fileName");
                    _MarketActionPic["PicPath"] = $(_imgList[i]).attr("data-ossPath");
                    _MarketActionPic["PicDesc"] = $(_imgList[i]).parent().parent().parent().find(".form-control").val();
                    _MarketActionPic["MarketActionId"] = $("#MarketActionId").val();
                    _MarketActionPicList.push(_MarketActionPic);
                }
            }
            return _MarketActionPicList;
        }

        function initAjax() {
            $.commonGet("Master/CoopFundTypeSearch", {
                coopFundId: '',
                coopFundName: '',
                coopFundNameEn: '',
                CoopFundType: '',
                showChk: 'true',
                modeType: 'MRN'
            }, function (data) {
                if (data) {
                    data.forEach(function (item) {
                        hdData.push({ text: isZH() ? item.CoopFundTypeName : item.CoopFundTypeNameEn, value: item.CoopFundTypeCode });
                    })
                }
            });
        }

        function submitAjax() {
            SProcessing();
            $.commonPost("MarketAction/DTTApproveSave", {
                MarketActionId: $("#MarketActionId").val(),
                DTTType: 2,
                DTTApproveCode: $("#DTTApproveCode").val(),
                DTTApproveDesc: $("#DTTApproveDesc").val(),
                modifyUserId: $("#G_UserId").val(),
                InUserId: $("#G_UserId").val()
            }, function () {
                EProcessing();
                if (roleName == "SHOP") {
                    //给DTT发邮件
                    if ($("#DTTApproveCode").val() != 2) {
                        if ($("#DTTApproveCode").val() != 2) {
                            $.commonGet("MarketAction/DMFApplyEmail", { marketActionId: $("#MarketActionId").val(), type: "MR" }, function (data) { });
                        }
                    }
                } else if (roleName == "BMC") {
                    //主视觉审批给谁发
                } else {
                    //给经销商发邮件
                    $.commonGet("MarketAction/DTTApproveEmail", { marketActionId: $("#MarketActionId").val(), type: "MR" }, function (data) { });
                }
                var msg = isZH() ? '提交成功！' : "Save successfully !";
                $('.form-control').prop("disabled", true);
                $('#DTTApproveCode').prop("disabled", true);
                layer.alert(msg, {
                    skin: 'layui-layer-molv', closeBtn: 0
                }, function (datas) {
                    window.location.href = "/Marketing/Index";
                });
            }, EProcessing)
        }

        function initImageList(_MarketActionBefore4WeeksPicList_OffLine) {
            for (let key in imgCodeObj) {
                initImageInfo(key, _MarketActionBefore4WeeksPicList_OffLine);
            }
        }

        function initImageInfo(imageCode, imageList) {
            let _imageList = [];
            for (let i = 0; i < imageList.length; i++) {
                if (imageList[i].PicType == imageCode) {
                    _imageList.push(imageList[i]);
                }
            }
            let _imgList = $("#" + imgCodeObj[imageCode]).find(".ossImage");
            for (let i = 0; i < _imageList.length; i++) {
                if ($(_imgList[i]).attr("data-flag") == 1) {
                    $(_imgList[i]).removeAttr("poster");
                    $(_imgList[i]).attr("src", baseOss + _imageList[i].PicPath);
                    $(_imgList[i]).attr("data-ossPath", _imageList[i].PicPath);
                    $(_imgList[i]).attr("data-flag", 2);
                    $(_imgList[i]).attr("data-fileName", _imageList[i].PicName);
                    $(_imgList[i]).parent().parent().find(".fileinput-exists").show();
                    $(_imgList[i]).parent().parent().parent().find(".form-control").val(_imageList[i].PicDesc);
                }
            }
        }

        var scheduleObj = { "schedule1": 0, "schedule2": 0, "schedule3": 0, "schedule4": 0, "schedule5": 0, "schedule6": 0, "schedule7": 0 };
        //验证逻辑
        function veriData(_type) {
            var MarketActionBefore21 = $('#PromotionCreate2').serializeObject();
            //活动报告信息验证
            let _schedule1 = true;
            if (!MarketActionBefore21.HightLights) {
                _schedule1 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写活动亮点！'
                    });
                    return false;
                }
            }
            if (!MarketActionBefore21.ImproveArea) {
                _schedule1 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写尚需改进处！'
                    });
                    return false;
                }
            }
            if (!MarketActionBefore21.CustomerFeedback) {
                _schedule1 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写客户反馈！'
                    });
                    return false;
                }
            }
            if (!MarketActionBefore21.MarketSaleTeamAdvice) {
                _schedule1 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写市场/销售团队建议！'
                    });
                    return false;
                }
            }
            if (_schedule1) {
                scheduleObj.schedule1 = 20;
            }

            //平台信息
            let _schedule2 = true;
            if (!MarketActionBefore21.Platform_Media) {
                _schedule2 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写媒体平台信息！'
                    });
                    return false;
                }
            }
            if (!MarketActionBefore21.Platform_ExposuerForm) {
                _schedule2 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写曝光形式信息！'
                    });
                    return false;
                }
            }
            if (_schedule2) {
                scheduleObj.schedule2 = 10;
            }

            //人员信息
            let _schedule3 = true;
            if (!veriNumber(MarketActionBefore21.People_NewLeadsThsYearCount)) {
                layer.open({
                    title: '错误提示',
                    type: 0,
                    content: '请正确填写线索数量！'
                });
                return false;
            }
            if (!MarketActionBefore21.People_NewLeadsThsYearCount) {
                _schedule3 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写线索数量！'
                    });
                    return false;
                }
            }
            if (!veriNumber(MarketActionBefore21.People_DCPIDCount)) {
                layer.open({
                    title: '错误提示',
                    type: 0,
                    content: '请正确填写DCPID客户数量！'
                });
                return false;
            }
            if (!MarketActionBefore21.People_DCPIDCount) {
                _schedule3 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写DCPID客户数量！'
                    });
                    return false;
                }
            }
            if (!veriNumber(MarketActionBefore21.People_NewOrderCount)) {
                layer.open({
                    title: '错误提示',
                    type: 0,
                    content: '请正确填写新增订单！'
                });
                return false;
            }
            if (!MarketActionBefore21.People_NewOrderCount) {
                _schedule3 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写新增订单！'
                    });
                    return false;
                }
            }
            if (!veriNumber(MarketActionBefore21.CoopFundSumAmt)) {
                layer.open({
                    title: '错误提示',
                    type: 0,
                    content: '请正确填写市场基金金额总计！'
                });
                return false;
            }
            if (!MarketActionBefore21.CoopFundSumAmt) {
                _schedule3 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写市场基金金额总计！'
                    });
                    return false;
                }
            }
            
            if (_schedule3) {
                scheduleObj.schedule3 = 10;
            }
            //活动实际费用清单
            var StrActivityFlow = $('#ActivityFlowTableOnline').bootstrapTable('getData');
            if (StrActivityFlow && StrActivityFlow.length > 0) {
                let _flowListVeri = [];
                let _flowDateVeri = true;
                StrActivityFlow.forEach(function (_item) {
                    let _flowVeri = false;
                    for (let _key in _item) {
                        if (_key != "SeqNO" && _item[_key]) {
                            _flowVeri = true;
                        }
                    }
                    _flowListVeri.push(_flowVeri);
                    //验证结束时间大于开始时间
                    if (_item["StartDate"] && _item["EndDate"]) {
                        let _startDate = new Date(_item["StartDate"]);
                        let _endDate = new Date(_item["EndDate"]);
                        if (_startDate.getTime() > _endDate.getTime()) {
                            layer.open({
                                title: '错误提示',
                                type: 0,
                                content: '投放开始时间不能大于等于投放结束时间！'
                            });
                            _flowDateVeri = false;
                        }
                    }
                });

                if (!_flowDateVeri) {
                    return false;
                }
                for (let i = 0; i < _flowListVeri.length; i++) {
                    if (!_flowListVeri[i]) {
                        layer.open({
                            title: '错误提示',
                            type: 0,
                            content: '市场基金申报信息填写不完整！'
                        });
                        return false;
                    }
                }
                let _sumAmt = 0;
                StrActivityFlow.forEach(function (item) {
                    //计算金额
                    if (item.CoopFund_DMFChk == "true" || item.CoopFund_DMFChk == true) {
                        _sumAmt += parseInt(item.CoopFundAmt);
                    }
                });
                let _CoopFundSumAmt = $("#CoopFundSumAmt").val();
                if (_CoopFundSumAmt > _sumAmt) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '市场基金金额总计应该小于报销总金额！'
                    });
                    return false;
                }
                scheduleObj.schedule5 = 20;
            } else {
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写市场基金申报信息相关信息！'
                    });
                    return false;
                }
            }
            //图片验证
            let _schedule7 = veriImage(_type);
            if (_schedule7) {
                scheduleObj.schedule7 = 40;
            } else {
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请上传相关报价单、发票和邮件截图！'
                    });
                    return false;
                }
            }
            return true;
        }

        //验证正整数正则
        function veriNumber(_data) {
            if (_data && _data > 0) {
                var reg = /^[1-9]\d*$/;
                return reg.test(_data);
            }
            return true;
        }

        //验证主视觉
        function veriKeyVisual(_data, _type) {
            let _schedule4 = true;
            if (_data.KeyVisionDesc) {
                _schedule4 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写主视觉描述！'
                    });
                }
            }
            if (_data.VisualDescription) {
                _schedule4 = false;
                if (_type == 2) {
                    layer.open({
                        title: '错误提示',
                        type: 0,
                        content: '请填写主视觉修改说明！'
                    });
                }
            }
            return _schedule4;
        }

        //图片验证
        function veriImage(_type) {
            let _schedule7 = true;
            let _MarketActionPicList = [];
            for (let _key in imgCodeObj) {
                if (_key == 'MRN01' || _key == 'MRN04' || _key == 'MRN02') {
                    let _imgList = $("#" + imgCodeObj[_key]).find(".ossImage");
                    let _veriImg = false;
                    for (let i = 0; i < _imgList.length; i++) {
                        if ($(_imgList[i]).attr("data-flag") == 2) {
                            _veriImg = true;
                        }
                    }
                    _MarketActionPicList.push(_veriImg);
                }
            }
            for (let i = 0; i < _MarketActionPicList.length; i++) {
                if (!_MarketActionPicList[i]) {
                    _schedule7 = false;
                }
            }
            return _schedule7;
        }

        //进度逻辑
        function planProgessSub() {
            let _sum = 0;
            for (let _key in scheduleObj) {
                _sum += parseInt(scheduleObj[_key]);
            }
            return _sum;
        }

        //生成PPT
        function createPPT(_type) {
            SProcessing();
            $.commonGet("MarketAction/CreatePPT", {
                marketActionId: $("#MarketActionId").val(),
                type: "MRN",
                userId: $("#G_UserId").val()
            }, function (data) {
                if (data.Status) {
                    if (_type) {
                        layer.open({
                            title: '提示',
                            type: 0,
                            content: '生成PPT成功！点击“下载PPT”即可下载！',
                            end: function () {
                                location.reload();
                            }
                        });
                    }
                } else {
                    if (_type) {
                        layer.open({
                            title: '提示',
                            type: 0,
                            content: '生成PPT失败！',
                            end: function () {
                                location.reload();
                            }
                        });
                    }
                }
                EProcessing();
            });
        }

        //下载PPT
        function downloadPPT() {
            SProcessing();
            $.commonGet("MarketAction/MarketActionPicSearch", {
                marketActionId: $("#MarketActionId").val(),
                picType: "MRN11"
